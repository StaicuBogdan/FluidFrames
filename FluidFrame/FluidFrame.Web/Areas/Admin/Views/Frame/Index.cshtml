<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Frames List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Frame" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> &nbsp; Create New Frame
            </a>
        </div>
    </div>
    <br /><br />

    <table id="tableData" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>ModelName</th>
                <th>Description</th>
                <th>Price</th>
                <th>OwnerPhoneNumber</th>
                <th>Category</th>
                <th>FrameType</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/frame.js"></script>
}